{
  "name": "Flutter Dev Container",
  "image": "mcr.miscrosoft.com/devcontainers/universal:2",
  "featuers": {
    "ghrc.io/devcontainers/feauters/flutter",
    "version": "latest"
  }
},
"customizations": {
  "vs-code": {
    "extensions": [
      "dart-code.flutter",
